<div class="d-flex">

  <div id="sidebar">
    <h4><strong> Welcome to your profile, <%= @helper.username %>! </strong></h4>
    <br>

    <div class="form-inline">
      <h5>Rating </h5>
     <%= image_tag "stars.png", id: "stars"%>
    </div>
    <br>

    <div class="helper-info">
      <p><a href="">Achievements</a></p>
      <p><a href="">Active tickets</a></p>
      <p><a href="">Tickets solved</a></p>
      <p><a href="">People helped</a></p>
      <p><a href="">Streak</a></p>

      <h4>Log out</h4>
    </div>


  </div>

  <div id="sidebar-right">
    <div class="d-flex justify-content-around align-items-center" id="right-of-sidebar">
      <h2>Devices:</h2>
      <%= image_tag "phone.png", :id => "start", width:'110px', height:'100px'%>
      <%= image_tag "ipad.png", :id => "start", width:'110px', height:'100px'%>
      <%= image_tag "laptop.png", :id => "start", width:'110px', height:'100px'%>
    </div>
    <hr>

    <h4> Tickets </h4>
    <div id="dashboard-ticket-container">

      <% @tickets.each do |ticket| %>
      <button class="accordion-dashboard"><%= ticket.description.split.first(3).join( " " ) + "..." %></button>
      <div class="panel">
        <p><%= ticket.description %></p>

        <div class="accordion-img">
        <%= image_tag "expand-img-btn.png", id: "dashboard-expand-img", width:'50px', height:'50px'%>
         <%= image_tag "play-btn.png", id: "dashboard-play-img", width:'50px', height:'50px'%>
         <!--need to change path to chat below for the button-->
         <%= link_to "Go to chat", new_ticket_path, class: "btn btn-yellow btn-yellow-accordion" %>
        </div>
        </div>
      <% end %>
    </div>
  </div>
</div><!--#d-flex-->

<!-- Button trigger modal -->
<button type="reset" data-toggle="modal" data-target="#exampleModal">
  Image button
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h2 class="modal-title" id="exampleModalLabel">Users issues</h2>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      <!-- Carousel -->
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="https://www.freevector.com/uploads/vector/preview/3990/FreeVector-No-Signal-TV.jpg"
              alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="data:image/jpeg;base64," alt="Second slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="https://media.npr.org/assets/img/2016/03/29/ap_090911089838_wide-58102eebb732a70864dc08c1ff9321af5051b9f4.jpg?s=1400" alt="Third slide">
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      <!-- End of Carousel -->
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
<!-- End of Modal -->
